Cell[CellGroupData[{

Cell["\<\
What\[CloseCurlyQuote]s the probability of winning (at least one) Teddy if \
you play twenty times?\
\>", "Subsection",
 CellChangeTimes->{3.7448108201908054`*^9},
 ExpressionUUID -> "3d32d4f0-3b34-4cd1-8f7e-ff421c4646ba"],

Cell[TextData[{
 "The probability of winning at least one in twenty tries was also calculated \
using the Law of Total Probability, as the probability of not winning \
anything in twenty tries can easily be calculated:\n\nBinomial Distribution \
with parameters n = 20, k= 0 and p = probSucess:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FormBox[
     TagBox[
      RowBox[{"(", GridBox[{
         {
          TagBox["n",
           Identity,
           Editable->True,
           Selectable->True]},
         {
          TagBox["k",
           Identity,
           Editable->True,
           Selectable->True]}
        }], ")"}],
      InterpretTemplate[Binomial[#, #2]& ],
      Editable->False,
      Selectable->False],
     TraditionalForm], 
    SuperscriptBox[
     RowBox[{
      SuperscriptBox["p", "k"], "(", 
      RowBox[{"1", "-", "p"}], ")"}], 
     RowBox[{"n", "-", "k"}]]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"029a860c-96ff-486d-b435-7e2244dbab92"],
 " "
}], "Text",
 CellChangeTimes->{{3.7454097632140894`*^9, 3.7454100459899426`*^9}, 
